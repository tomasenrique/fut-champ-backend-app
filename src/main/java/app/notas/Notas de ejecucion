# COORDINADOR

POST http://localhost:8080/api/futchamp/coordinador/agregar
Content-Type: application/json

{
  "nombre": "Luis alberto",
  "apellidos": "arriaga calderon",
  "dni": "A01478269K",
  "genero": "H",
  "fNac": "1969-02-17",
  "email": "luisAlberto@hotmail.com",
  "telefono": "666258106",
  "imagen": "www.urlIgamenCoordinador.com",
  "ocupacion": "coordinador",
  "cargo": "jefe",
  "equipo": {
    "nombre": "los gatos"
  }
}

###

GET http://localhost:8080/api/futchamp/coordinador/mostrar

###

GET http://localhost:8080/api/futchamp/coordinador/mostrar/idEquipo/2

###

GET http://localhost:8080/api/futchamp/coordinador/mostrar/dni/12345678B

###

PUT http://localhost:8080/api/futchamp/coordinador/actualizar
Content-Type: application/json

{
  "id": 1,
  "nombre": "Luis alberto",
  "apellidos": "arriaga calderon",
  "dni": "A01478269K",
  "genero": "H",
  "fNac": "1969-02-17",
  "email": "luisAlberto@hotmail.com",
  "telefono": "666258106",
  "imagen": "www.urlIgamenCoordinador.com",
  "ocupacion": "coordinador",
  "cargo": "jefe",
  "equipo": {
    "nombre": "los gatos"
  }
}

###

### Este enlace actualiza la clave del coordinador
PUT http://localhost:8080/api/futchamp/coordinador/actualizar/clave?idCoordinador=15&nuevaClave=x9893913a




###




########################################################################################################################
# EQUIPO
POST http://localhost:8080/api/futchamp/equipo/agregar
Content-Type: application/json

{
  "nombre": "Los pececitos",
  "categoria": "medianos",
  "logo": "www.urlImagemLogo.com",
  "liga": {
    "nombre": "proyecto los jugadores"
  }
}

###

GET http://localhost:8080/api/futchamp/equipo/mostrar

###

GET http://localhost:8080/api/futchamp/equipo/mostrar/categoria/medianos

###

GET http://localhost:8080/api/futchamp/equipo/mostrar/nombre/los pececitos

###

PUT http://localhost:8080/api/futchamp/equipo/actualizar
Content-Type: application/json

{
  "id": 4,
  "nombre": "Los pececitos grandes",
  "categoria": "grandes",
  "logo": "www.urlImagemLogo.com"
}

###

DELETE http://localhost:8080/api/futchamp/equipo/eliminar/4



